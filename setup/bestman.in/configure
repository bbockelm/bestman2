Ver_separator ()
{
	var1=$1
	if test ! "$var1" ; then
		var1=0
	fi
	var2=$2
	if test ! "$var2" ; then
		var2=0
	fi
	var3=$3
	if test ! "$var3" ; then
		var3=0
	fi
}

Ver_getter ()
{
	varget=$1
	varget=`echo $varget | sed 's/\./ /g'`
}


