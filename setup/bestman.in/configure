if test "X$BUILDP" = "XNOBUILD"; then
   mJARNAME=
   mJARPATH=
else
   mJARNAME=`grep $AXIS $BUILDP | tr '=' '\n' | sed -e 's/^.*\///'  | grep "^$AXIS.*.jar"`
   mJARPATH=`grep $mJARNAME $BUILDP | tr '=' '\n' | grep "/$mJARNAME"`
fi
if test "X$mJARPATH" = "X" ; then
   mJARNAME=`echo $MYCLASSPATH | tr ':' '\n' | sed -e 's/^.*\///'  | grep "^$AXIS.*.jar"`
   AXISNAME=$mJARNAME
   if test "$mJARNAME" ; then
      mJARPATH=`echo $MYCLASSPATH | tr ':' '\n' | grep "/$mJARNAME"`
      AXISPATH=$mJARPATH
      if test -f "$mJARPATH" ; then
         { $as_echo "$as_me:${as_lineno-$LINENO}: result: VALIDATED: $AXIS $mJARNAME for version $AXISVERSION is found in $mJARPATH." >&5
$as_echo "VALIDATED: $AXIS $mJARNAME for version $AXISVERSION is found in $mJARPATH." >&6; }
      else
         as_fn_error "FAILED: $AXIS $mJARNAME for version $AXISVERSION is not found in $mJARPATH." "$LINENO" 5
      fi
   else
      as_fn_error "FAILED: $AXIS $AXISVERSION is not found in $BUILDP nor in CLASSPATH $MYCLASSPATH." "$LINENO" 5
   fi
else
   AXISNAME=$mJARNAME
   AXISPATH=$mJARPATH
   { $as_echo "$as_me:${as_lineno-$LINENO}: result: VALIDATED: $AXIS $mJARNAME for version $AXISVERSION is found in $mJARPATH." >&5
$as_echo "VALIDATED: $AXIS $mJARNAME for version $AXISVERSION is found in $mJARPATH." >&6; }
fi


