fi

        else

if test -f "$JAVA_HOME/bin/java" -a -d "$JAVA_HOME"; then
	j_ver=`$JAVA_HOME/bin/java -version 2>&1`

	j_ver=`echo $j_ver | sed 's/[^0-9. ]//g'`

	Ver_getter $j_ver
	Ver_separator $varget
	ver=
	echo "current java version is $var1.$var2.$var3"

if test "1" -lt "$var1" ; then
	ver=yes
elif test "1" -eq "$var1" -a "6" -lt "$var2" ; then
	ver=yes
elif test "1" -eq "$var1" -a "6" -eq "$var2" -a "001" -le "$var3" ; then
	ver=yes
else
	ver=no
fi

	{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for JAVA version ( > 1.6.0_01)" >&5
$as_echo_n "checking for JAVA version ( > 1.6.0_01)... " >&6; }
	if test "$ver" = "yes" ; then
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: ok" >&5
$as_echo "ok" >&6; }
	else
		{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: invalid JAVA specified" >&5
$as_echo "$as_me: WARNING: invalid JAVA specified" >&2;}
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: higher version needed" >&5
$as_echo "higher version needed" >&6; }
	    as_fn_error "usable java version not found" "$LINENO" 5
	fi
else
	as_fn_error "java not found" "$LINENO" 5
fi

        fi
    fi
fi


